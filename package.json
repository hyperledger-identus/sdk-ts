  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "workerThreads": true,
    "maxWorkers": 1,
    "transformIgnorePatterns": [
      "node_modules/(?!@noble)"
    ],
    "automock": false,
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "moduleNameMapper": {
      "^core-js/modules/(.*)$": "<rootDir>/node_modules/core-js/modules/$1"
    },
    "coverageReporters": [
      "json-summary",
      "lcov",
      "text"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coveragePathIgnorePatterns": [
      "tests",
      "anoncreds-rust",
      "didcomm-rust"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 22,
        "functions": 33,
        "lines": 34,
        "statements": 34
      }
    },
    "coverageDirectory": "coverage"
  },
  "browserslist": [
    "last 2 chrome version",
    "last 2 firefox version",
    "last 2 safari version",
    "last 2 edge version"
  ],
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "junit.xml"
  },
