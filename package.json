{
  "name": "@input-output-hk/atala-prism-sdk",
  "version": "2.0.0",
  "description": "A ready to use library including the emurgo wasm for Cardano and a set of utilities to encrypt, decrypt, sign and verify anything using native wasm",
  "main": "src/index.ts",
  "exports": {
    "./node": {
      "require": "./build/node/cjs/index.js",
      "import": "./build/node/es/index.js"
    },
    "./browser": {
      "require": "./build/browser/cjs/index.js",
      "import": "./build/browser/es/index.js"
    }
  },
  "files": [
    "src",
    "node",
    "browser",
    "index.ts",
    "apollo",
    "castor",
    "config",
    "domain",
    "mercury",
    "pluto",
    "pollux",
    "prism-agent"
  ],
  "scripts": {
    "build": "rm -rf build && npm run build:node && npm run build:node-esm && npm run build:browser && npm run build:browser-esm",
    "build:node": "rm -rf build/node/cjs && npx rollup -c rollup/node/rollup.cjs.js",
    "build:node-esm": "rm -rf build/node/es && npx rollup -c rollup/node/rollup.es.js",
    "build:browser": "rm -rf build/browser/cjs && npx rollup -c rollup/browser/rollup.cjs.js",
    "build:browser-esm": "rm -rf build/browser/es && npx rollup -c rollup/browser/rollup.es.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'tests/**/*.ts'",
    "amtlr": "./node_modules/.bin/antlr4ts ./castor/parser/DIDAbnf.g4  -long-messages  && ./node_modules/.bin/antlr4ts  -long-messages ./castor/parser/DIDUrlAbnf.g4",
    "protos": "rm -rf protos/generated && mkdir -p protos/generated && yarn protos:node && yarn protos:common",
    "protos:node": "protoc -Iprotos --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --ts_out=\"./protos/generated\" --js_out=\"import_style=commonjs,binary:./protos/generated\"  protos/node_models.proto",
    "protos:common": "protoc -Iprotos --plugin=\"protoc-gen-ts=./node_modules/.bin/protoc-gen-ts\" --ts_out=\"./protos/generated\" --js_out=\"import_style=commonjs,binary:./protos/generated\"  protos/common_models.proto"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/plugin-transform-typescript": "^7.15.4",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime-corejs3": "^7.15.4",
    "@input-output-hk/atala-prism-protos": "1.4.1",
    "@input-output-hk/atala-prism-sdk": "1.4.1",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-wasm": "^5.1.2",
    "@types/chai": "^4.3.4",
    "@types/elliptic": "^6.4.14",
    "@types/google-protobuf": "^3.15.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.14.1",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^9.0.1",
    "antlr4ts-cli": "^0.5.0-alpha.4",
    "arraybuffer-loader": "^1.0.8",
    "babel-loader": "^8.2.2",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-plugin-nameof-js": "^0.0.2",
    "bn.js": "^5.2.1",
    "chai": "^4.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "crypto": "^1.0.1",
    "crypto-browserify": "^3.12.0",
    "event": "^1.0.0",
    "file-loader": "^6.2.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "pbkdf2": "^3.1.2",
    "process": "^0.11.10",
    "puppeteer": "^1.13.0",
    "rollup": "^2.68.0",
    "rollup-plugin-cleanup": "^3.2.1",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-ignore": "^1.0.10",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.34.1",
    "run-all": "^1.0.1",
    "sinon": "^15.0.1",
    "stream-browserify": "^3.0.0",
    "ts-node": "^10.8.1",
    "ts-protoc-gen": "^0.15.0",
    "typescript": "^4.2.3",
    "util": "^0.12.4",
    "wasm-loader": "^1.3.0"
  },
  "keywords": [],
  "author": "Javier Rib√≥",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4",
    "bip39": "^3.1.0",
    "elliptic": "^6.5.4",
    "proto3-json-serializer": "^1.1.0",
    "uuid": "^9.0.0"
  }
}
